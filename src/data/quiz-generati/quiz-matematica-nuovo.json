    {"id": "mat-001", "domanda": "Qual è il dominio della funzione f(x) = √(x-2)?", "tipo": "multipla", "opzioni": ["x ≥ 2", "x > 2", "x ≤ 2", "x ∈ ℝ"], "corretta": 0, "spiegazione": "Per le radici pari, il radicando deve essere ≥ 0.", "difficolta": "facile", "argomento": "Funzioni reali", "sottoargomento": "Dominio, codominio, e grafico"},
    {"id": "mat-002", "domanda": "Qual è il dominio di f(x) = 1/(x²-4)?", "tipo": "multipla", "opzioni": ["ℝ - {-2, 2}", "ℝ - {2}", "ℝ - {4}", "ℝ"], "corretta": 0, "spiegazione": "Il denominatore x²-4 ≠ 0, quindi x ≠ ±2.", "difficolta": "facile", "argomento": "Funzioni reali", "sottoargomento": "Dominio, codominio, e grafico"},
    {"id": "mat-003", "domanda": "Una funzione f è iniettiva se:", "tipo": "multipla", "opzioni": ["f(x₁) = f(x₂) implica x₁ = x₂", "Ogni y ha controimmagine", "f(x) > 0", "Dom = Cod"], "corretta": 0, "spiegazione": "Iniettiva: valori diversi producono immagini diverse.", "difficolta": "intermedio", "argomento": "Funzioni reali", "sottoargomento": "Dominio, codominio, e grafico"},
    {"id": "mat-004", "domanda": "Il dominio di log(x) è:", "tipo": "multipla", "opzioni": ["(0, +∞)", "[0, +∞)", "ℝ", "ℝ - {0}"], "corretta": 0, "spiegazione": "Il logaritmo è definito solo per x > 0.", "difficolta": "facile", "argomento": "Funzioni reali", "sottoargomento": "Dominio, codominio, e grafico"},
    {"id": "mat-005", "domanda": "Una funzione biiettiva è:", "tipo": "multipla", "opzioni": ["Iniettiva e suriettiva", "Solo iniettiva", "Solo suriettiva", "Periodica"], "corretta": 0, "spiegazione": "Biiettiva = iniettiva + suriettiva.", "difficolta": "intermedio", "argomento": "Funzioni reali", "sottoargomento": "Dominio, codominio, e grafico"},
    {"id": "mat-006", "domanda": "(g∘f)(x) significa:", "tipo": "multipla", "opzioni": ["g(f(x))", "f(g(x))", "g·f", "g+f"], "corretta": 0, "spiegazione": "Composizione: prima f, poi g.", "difficolta": "intermedio", "argomento": "Funzioni reali", "sottoargomento": "Dominio, codominio, e grafico"},
    {"id": "mat-007", "domanda": "f(x)+3 trasla il grafico:", "tipo": "multipla", "opzioni": ["Verso l'alto di 3", "Verso destra di 3", "Verso il basso", "Non lo trasla"], "corretta": 0, "spiegazione": "+k trasla verso l'alto.", "difficolta": "facile", "argomento": "Funzioni reali", "sottoargomento": "Funzioni elementari, e trasformazioni"},
    {"id": "mat-008", "domanda": "f(x-2) trasla il grafico:", "tipo": "multipla", "opzioni": ["Verso destra di 2", "Verso sinistra di 2", "Verso l'alto", "Verso il basso"], "corretta": 0, "spiegazione": "f(x-h) trasla a destra di h.", "difficolta": "intermedio", "argomento": "Funzioni reali", "sottoargomento": "Funzioni elementari, e trasformazioni"},
    {"id": "mat-009", "domanda": "f è pari se:", "tipo": "multipla", "opzioni": ["f(-x) = f(x)", "f(-x) = -f(x)", "f(x+T) = f(x)", "f(x) > 0"], "corretta": 0, "spiegazione": "Pari = simmetrica rispetto all'asse y.", "difficolta": "facile", "argomento": "Funzioni reali", "sottoargomento": "Funzioni elementari, e trasformazioni"},
    {"id": "mat-010", "domanda": "f è dispari se:", "tipo": "multipla", "opzioni": ["f(-x) = -f(x)", "f(-x) = f(x)", "f(x) < 0", "f periodica"], "corretta": 0, "spiegazione": "Dispari = simmetrica rispetto all'origine.", "difficolta": "facile", "argomento": "Funzioni reali", "sottoargomento": "Funzioni elementari, e trasformazioni"},
    {"id": "mat-011", "domanda": "-f(x) ribalta il grafico:", "tipo": "multipla", "opzioni": ["Rispetto all'asse x", "Rispetto all'asse y", "Rispetto all'origine", "Non lo ribalta"], "corretta": 0, "spiegazione": "Moltiplicare per -1 ribalta rispetto all'asse x.", "difficolta": "intermedio", "argomento": "Funzioni reali", "sottoargomento": "Funzioni elementari, e trasformazioni"},
    {"id": "mat-012", "domanda": "L'immagine di eˣ è:", "tipo": "multipla", "opzioni": ["(0, +∞)", "ℝ", "[0, +∞)", "(-∞, 0)"], "corretta": 0, "spiegazione": "eˣ è sempre positiva.", "difficolta": "facile", "argomento": "Funzioni reali", "sottoargomento": "Funzioni elementari, e trasformazioni"},
    {"id": "mat-013", "domanda": "|f(x)| rispetto a f(x):", "tipo": "multipla", "opzioni": ["Ribalta la parte negativa sopra l'asse x", "Ribalta rispetto all'asse y", "Trasla verso l'alto", "Non cambia"], "corretta": 0, "spiegazione": "Il valore assoluto rende positivi tutti i valori.", "difficolta": "intermedio", "argomento": "Funzioni reali", "sottoargomento": "Funzioni elementari, e trasformazioni"},
    {"id": "mat-014", "domanda": "lim(x→2) f(x) = 5 significa:", "tipo": "multipla", "opzioni": ["f(x) → 5 quando x → 2", "f(2) = 5", "f(5) = 2", "f = 5"], "corretta": 0, "spiegazione": "Il limite descrive il comportamento vicino al punto.", "difficolta": "facile", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-015", "domanda": "lim(x→0) sin(x)/x =", "tipo": "multipla", "opzioni": ["1", "0", "+∞", "Non esiste"], "corretta": 0, "spiegazione": "Limite notevole fondamentale.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-016", "domanda": "0/0 è:", "tipo": "multipla", "opzioni": ["Forma indeterminata", "= 0", "= 1", "= ∞"], "corretta": 0, "spiegazione": "Richiede ulteriori manipolazioni.", "difficolta": "facile", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-017", "domanda": "lim(x→+∞) (1+1/x)ˣ =", "tipo": "multipla", "opzioni": ["e", "1", "+∞", "0"], "corretta": 0, "spiegazione": "Definizione del numero e.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-018", "domanda": "lim(x→0) (eˣ-1)/x =", "tipo": "multipla", "opzioni": ["1", "e", "0", "+∞"], "corretta": 0, "spiegazione": "Limite notevole.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-019", "domanda": "Il limite esiste se:", "tipo": "multipla", "opzioni": ["Limiti dx e sx coincidono", "Limite dx esiste", "f definita nel punto", "f continua"], "corretta": 0, "spiegazione": "Devono coincidere i limiti laterali.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-020", "domanda": "f continua in x₀ se:", "tipo": "multipla", "opzioni": ["lim f(x) = f(x₀)", "f(x₀) esiste", "lim esiste", "f derivabile"], "corretta": 0, "spiegazione": "Continuità: limite = valore.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Continuità, e teoremi fondamentali"},
    {"id": "mat-021", "domanda": "Discontinuità di salto:", "tipo": "multipla", "opzioni": ["Limiti laterali diversi ma finiti", "Limite infinito", "f non definita", "Limiti uguali ≠ f(x₀)"], "corretta": 0, "spiegazione": "Salto = gradino tra i limiti.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Continuità, e teoremi fondamentali"},
    {"id": "mat-022", "domanda": "Teorema zeri: f continua, f(a)·f(b)<0 →", "tipo": "multipla", "opzioni": ["∃c: f(c)=0", "f ha massimo", "f derivabile", "f costante"], "corretta": 0, "spiegazione": "Se cambia segno, attraversa l'asse x.", "difficolta": "avanzato", "argomento": "Limiti, e continuità", "sottoargomento": "Continuità, e teoremi fondamentali"},
    {"id": "mat-023", "domanda": "Weierstrass: f continua su [a,b]:", "tipo": "multipla", "opzioni": ["Ha max e min assoluti", "È derivabile", "Ha uno zero", "È monotona"], "corretta": 0, "spiegazione": "Su intervallo chiuso, f assume i suoi estremi.", "difficolta": "avanzato", "argomento": "Limiti, e continuità", "sottoargomento": "Continuità, e teoremi fondamentali"},
    {"id": "mat-024", "domanda": "Derivabilità implica:", "tipo": "multipla", "opzioni": ["Continuità", "Monotonia", "Massimi", "Limitatezza"], "corretta": 0, "spiegazione": "Derivabile → continua.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Continuità, e teoremi fondamentali"},
    {"id": "mat-025", "domanda": "f'(x₀) geometricamente è:", "tipo": "multipla", "opzioni": ["Coeff. angolare tangente", "Area sotto grafico", "Valore funzione", "Lunghezza arco"], "corretta": 0, "spiegazione": "Derivata = pendenza tangente.", "difficolta": "facile", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-026", "domanda": "(xⁿ)' =", "tipo": "multipla", "opzioni": ["n·xⁿ⁻¹", "xⁿ⁺¹/(n+1)", "n·xⁿ", "xⁿ/n"], "corretta": 0, "spiegazione": "Regola delle potenze.", "difficolta": "facile", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-027", "domanda": "(eˣ)' =", "tipo": "multipla", "opzioni": ["eˣ", "x·eˣ⁻¹", "eˣ/x", "ln(x)"], "corretta": 0, "spiegazione": "eˣ è uguale alla sua derivata.", "difficolta": "facile", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-028", "domanda": "(sin x)' =", "tipo": "multipla", "opzioni": ["cos x", "-cos x", "sin x", "-sin x"], "corretta": 0, "spiegazione": "Derivata del seno è il coseno.", "difficolta": "facile", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-029", "domanda": "(cos x)' =", "tipo": "multipla", "opzioni": ["-sin x", "sin x", "cos x", "-cos x"], "corretta": 0, "spiegazione": "Derivata del coseno è -seno.", "difficolta": "facile", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-030", "domanda": "(ln x)' =", "tipo": "multipla", "opzioni": ["1/x", "x", "eˣ", "ln x/x"], "corretta": 0, "spiegazione": "Derivata del logaritmo naturale.", "difficolta": "facile", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-031", "domanda": "(fg)' =", "tipo": "multipla", "opzioni": ["f'g + fg'", "f'g'", "f'g - fg'", "fg"], "corretta": 0, "spiegazione": "Regola del prodotto.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-032", "domanda": "(f/g)' =", "tipo": "multipla", "opzioni": ["(f'g - fg')/g²", "(f'g + fg')/g²", "f'/g'", "f'g - fg'"], "corretta": 0, "spiegazione": "Regola del quoziente.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-033", "domanda": "[f(g(x))]' =", "tipo": "multipla", "opzioni": ["f'(g(x))·g'(x)", "f'(x)·g'(x)", "f(g'(x))", "f'(g'(x))"], "corretta": 0, "spiegazione": "Regola della catena.", "difficolta": "avanzato", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-034", "domanda": "(tan x)' =", "tipo": "multipla", "opzioni": ["1/cos²x", "1/sin²x", "sin x/cos x", "-1/cos²x"], "corretta": 0, "spiegazione": "Derivata della tangente.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-035", "domanda": "Punto critico: f'(x) =", "tipo": "multipla", "opzioni": ["0 o non esiste", "f(x) = 0", "f''(x) = 0", "f discontinua"], "corretta": 0, "spiegazione": "Punti critici: derivata nulla o non esistente.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Teoremi fondamentali calcolo differenziale"},
    {"id": "mat-036", "domanda": "Fermat: x₀ estremo interno, f derivabile →", "tipo": "multipla", "opzioni": ["f'(x₀) = 0", "f(x₀) = 0", "f''(x₀) = 0", "f costante"], "corretta": 0, "spiegazione": "Negli estremi interni la tangente è orizzontale.", "difficolta": "avanzato", "argomento": "Derivate, e applicazioni", "sottoargomento": "Teoremi fondamentali calcolo differenziale"},
    {"id": "mat-037", "domanda": "Se f'(x) > 0, f è:", "tipo": "multipla", "opzioni": ["Crescente", "Decrescente", "Costante", "Concava"], "corretta": 0, "spiegazione": "Derivata positiva → crescente.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Teoremi fondamentali calcolo differenziale"},
    {"id": "mat-038", "domanda": "De L'Hôpital si applica a:", "tipo": "multipla", "opzioni": ["0/0 e ∞/∞", "0·∞", "1^∞", "Tutte"], "corretta": 0, "spiegazione": "Solo a 0/0 e ∞/∞ direttamente.", "difficolta": "avanzato", "argomento": "Derivate, e applicazioni", "sottoargomento": "Teoremi fondamentali calcolo differenziale"},
    {"id": "mat-039", "domanda": "Lagrange: ∃c tale che f'(c) =", "tipo": "multipla", "opzioni": ["[f(b)-f(a)]/(b-a)", "f(b)-f(a)", "0", "f(a)+f(b)"], "corretta": 0, "spiegazione": "Tangente parallela alla secante.", "difficolta": "avanzato", "argomento": "Derivate, e applicazioni", "sottoargomento": "Teoremi fondamentali calcolo differenziale"},
    {"id": "mat-040", "domanda": "Se f''(x) > 0, f è:", "tipo": "multipla", "opzioni": ["Convessa (∪)", "Concava (∩)", "Crescente", "Decrescente"], "corretta": 0, "spiegazione": "f'' > 0 → concavità verso l'alto.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Studio completo di funzione"},
    {"id": "mat-041", "domanda": "Flesso: punto dove:", "tipo": "multipla", "opzioni": ["Cambia concavità", "f'(x) = 0", "f(x) = 0", "f discontinua"], "corretta": 0, "spiegazione": "Nel flesso cambia la concavità.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Studio completo di funzione"},
    {"id": "mat-042", "domanda": "Asintoto verticale x=a se:", "tipo": "multipla", "opzioni": ["lim f(x) = ±∞ per x→a", "f(a) = ∞", "f'(a) = ∞", "f continua in a"], "corretta": 0, "spiegazione": "La funzione diverge avvicinandosi ad a.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Studio completo di funzione"},
    {"id": "mat-043", "domanda": "Asintoto orizzontale y=L se:", "tipo": "multipla", "opzioni": ["lim f(x) = L per x→±∞", "f(L) = 0", "f'(x) = L", "f costante"], "corretta": 0, "spiegazione": "Comportamento all'infinito.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Studio completo di funzione"},
    {"id": "mat-044", "domanda": "Coeff. m asintoto obliquo:", "tipo": "multipla", "opzioni": ["lim f(x)/x per x→∞", "lim f(x)", "f'(∞)", "f(0)"], "corretta": 0, "spiegazione": "m = lim f(x)/x, poi q = lim[f(x)-mx].", "difficolta": "avanzato", "argomento": "Derivate, e applicazioni", "sottoargomento": "Studio completo di funzione"},
    {"id": "mat-045", "domanda": "Primitiva F di f: F'(x) =", "tipo": "multipla", "opzioni": ["f(x)", "F(x)", "∫f(x)dx", "f'(x)"], "corretta": 0, "spiegazione": "La primitiva ha derivata uguale a f.", "difficolta": "facile", "argomento": "Integrali", "sottoargomento": "Integrale indefinito, e primitive"},
    {"id": "mat-046", "domanda": "∫xⁿ dx (n≠-1) =", "tipo": "multipla", "opzioni": ["xⁿ⁺¹/(n+1) + c", "n·xⁿ⁻¹ + c", "xⁿ/n + c", "xⁿ⁺¹ + c"], "corretta": 0, "spiegazione": "Integrale delle potenze.", "difficolta": "facile", "argomento": "Integrali", "sottoargomento": "Integrale indefinito, e primitive"},
    {"id": "mat-047", "domanda": "∫1/x dx =", "tipo": "multipla", "opzioni": ["ln|x| + c", "x⁻¹ + c", "-1/x² + c", "eˣ + c"], "corretta": 0, "spiegazione": "Integrale di 1/x.", "difficolta": "facile", "argomento": "Integrali", "sottoargomento": "Integrale indefinito, e primitive"},
    {"id": "mat-048", "domanda": "∫eˣ dx =", "tipo": "multipla", "opzioni": ["eˣ + c", "x·eˣ + c", "eˣ/x + c", "ln(eˣ) + c"], "corretta": 0, "spiegazione": "eˣ è primitiva di se stessa.", "difficolta": "facile", "argomento": "Integrali", "sottoargomento": "Integrale indefinito, e primitive"},
    {"id": "mat-049", "domanda": "∫cos x dx =", "tipo": "multipla", "opzioni": ["sin x + c", "-sin x + c", "cos x + c", "-cos x + c"], "corretta": 0, "spiegazione": "Primitiva del coseno.", "difficolta": "facile", "argomento": "Integrali", "sottoargomento": "Integrale indefinito, e primitive"},
    {"id": "mat-050", "domanda": "∫sin x dx =", "tipo": "multipla", "opzioni": ["-cos x + c", "cos x + c", "sin x + c", "-sin x + c"], "corretta": 0, "spiegazione": "Primitiva del seno.", "difficolta": "facile", "argomento": "Integrali", "sottoargomento": "Integrale indefinito, e primitive"},
    {"id": "mat-051", "domanda": "Integrazione per parti: ∫f·g' =", "tipo": "multipla", "opzioni": ["f·g - ∫f'·g", "f'·g'", "∫f/g", "f'·g"], "corretta": 0, "spiegazione": "Formula dell'integrazione per parti.", "difficolta": "intermedio", "argomento": "Integrali", "sottoargomento": "Integrale indefinito, e primitive"},
    {"id": "mat-052", "domanda": "∫[a,b] f(x)dx geometricamente:", "tipo": "multipla", "opzioni": ["Area con segno sotto il grafico", "Derivata in [a,b]", "Valore medio", "Lunghezza grafico"], "corretta": 0, "spiegazione": "Integrale definito = area algebrica.", "difficolta": "facile", "argomento": "Integrali", "sottoargomento": "Integrale definito, e teorema fondamentale"},
    {"id": "mat-053", "domanda": "Teorema fondamentale: ∫[a,b] f dx =", "tipo": "multipla", "opzioni": ["F(b) - F(a)", "F(a) - F(b)", "F(b) + F(a)", "F(b)·F(a)"], "corretta": 0, "spiegazione": "Integrale = differenza primitive agli estremi.", "difficolta": "intermedio", "argomento": "Integrali", "sottoargomento": "Integrale definito, e teorema fondamentale"},
    {"id": "mat-054", "domanda": "Se F(x) = ∫[a,x] f(t)dt, F'(x) =", "tipo": "multipla", "opzioni": ["f(x)", "F(x)", "∫f dx", "f(a)"], "corretta": 0, "spiegazione": "Derivata dell'integrale = integranda.", "difficolta": "avanzato", "argomento": "Integrali", "sottoargomento": "Integrale definito, e teorema fondamentale"},
    {"id": "mat-055", "domanda": "Volume rotazione asse x:", "tipo": "multipla", "opzioni": ["∫π[f(x)]² dx", "∫f(x) dx", "∫2πf(x) dx", "π∫f(x) dx"], "corretta": 0, "spiegazione": "Metodo dei dischi: π·r².", "difficolta": "avanzato", "argomento": "Integrali", "sottoargomento": "Integrale definito, e teorema fondamentale"},
    {"id": "mat-056", "domanda": "Area tra f e g (f≥g):", "tipo": "multipla", "opzioni": ["∫[f(x)-g(x)] dx", "∫f·g dx", "∫f dx - ∫g dx", "∫[f+g] dx"], "corretta": 0, "spiegazione": "Area = integrale della differenza.", "difficolta": "intermedio", "argomento": "Integrali", "sottoargomento": "Integrale definito, e teorema fondamentale"},
    {"id": "mat-057", "domanda": "Permutazioni di n oggetti:", "tipo": "multipla", "opzioni": ["n!", "nⁿ", "2ⁿ", "n²"], "corretta": 0, "spiegazione": "P_n = n! = n·(n-1)·...·1.", "difficolta": "facile", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-058", "domanda": "C(n,k) rappresenta:", "tipo": "multipla", "opzioni": ["Combinazioni di k da n", "Permutazioni", "n·k", "n^k"], "corretta": 0, "spiegazione": "Combinazioni: scelte senza ordine.", "difficolta": "intermedio", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-059", "domanda": "P(evento certo) =", "tipo": "multipla", "opzioni": ["1", "0", "0.5", "∞"], "corretta": 0, "spiegazione": "Evento certo ha probabilità 1.", "difficolta": "facile", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-060", "domanda": "Eventi indipendenti: P(A∩B) =", "tipo": "multipla", "opzioni": ["P(A)·P(B)", "P(A)+P(B)", "P(A|B)", "P(A∪B)"], "corretta": 0, "spiegazione": "Indipendenza: prodotto delle probabilità.", "difficolta": "intermedio", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-061", "domanda": "P(A|B) =", "tipo": "multipla", "opzioni": ["P(A∩B)/P(B)", "P(A)·P(B)", "P(A∪B)/P(B)", "P(A)/P(B)"], "corretta": 0, "spiegazione": "Probabilità condizionata.", "difficolta": "intermedio", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-062", "domanda": "Bayes permette di calcolare:", "tipo": "multipla", "opzioni": ["P(A|B) da P(B|A)", "P(A∪B)", "P(A∩B)", "P(A)+P(B)"], "corretta": 0, "spiegazione": "Bayes inverte il condizionamento.", "difficolta": "avanzato", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-063", "domanda": "P(A∪B) =", "tipo": "multipla", "opzioni": ["P(A)+P(B)-P(A∩B)", "P(A)+P(B)", "P(A)·P(B)", "P(A)-P(B)"], "corretta": 0, "spiegazione": "Formula probabilità totale.", "difficolta": "intermedio", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-064", "domanda": "E(X) variabile discreta:", "tipo": "multipla", "opzioni": ["Σ x·P(X=x)", "Σ P(X=x)", "max(X)", "Σ x²·P(X=x)"], "corretta": 0, "spiegazione": "Valore atteso = media pesata.", "difficolta": "intermedio", "argomento": "Probabilità, e statistica", "sottoargomento": "Variabili aleatorie, e distribuzioni"},
    {"id": "mat-065", "domanda": "Var(X) misura:", "tipo": "multipla", "opzioni": ["Dispersione intorno alla media", "Valore medio", "Valore massimo", "Probabilità"], "corretta": 0, "spiegazione": "Varianza = dispersione.", "difficolta": "intermedio", "argomento": "Probabilità, e statistica", "sottoargomento": "Variabili aleatorie, e distribuzioni"},
    {"id": "mat-066", "domanda": "Distribuzione normale ha forma:", "tipo": "multipla", "opzioni": ["Campana simmetrica", "Uniforme", "Esponenziale", "A gradini"], "corretta": 0, "spiegazione": "Gaussiana = campana.", "difficolta": "facile", "argomento": "Probabilità, e statistica", "sottoargomento": "Variabili aleatorie, e distribuzioni"},
    {"id": "mat-067", "domanda": "Teorema limite centrale:", "tipo": "multipla", "opzioni": ["Somma variabili → normale", "Ogni variabile è normale", "Media = 0", "Varianza = 1"], "corretta": 0, "spiegazione": "Somma di molte variabili tende alla normale.", "difficolta": "avanzato", "argomento": "Probabilità, e statistica", "sottoargomento": "Variabili aleatorie, e distribuzioni"},
    {"id": "mat-068", "domanda": "σ (deviazione standard) =", "tipo": "multipla", "opzioni": ["√Var(X)", "Var(X)²", "E(X)", "Var(X)/n"], "corretta": 0, "spiegazione": "σ = radice della varianza.", "difficolta": "facile", "argomento": "Probabilità, e statistica", "sottoargomento": "Variabili aleatorie, e distribuzioni"},
    {"id": "mat-069", "domanda": "Binomiale B(n,p): E(X) =", "tipo": "multipla", "opzioni": ["n·p", "n/p", "p/n", "n+p"], "corretta": 0, "spiegazione": "Valore atteso binomiale.", "difficolta": "intermedio", "argomento": "Probabilità, e statistica", "sottoargomento": "Variabili aleatorie, e distribuzioni"},
    {"id": "mat-070", "domanda": "D(5,3) disposizioni =", "tipo": "multipla", "opzioni": ["60", "10", "15", "125"], "corretta": 0, "spiegazione": "D(5,3) = 5·4·3 = 60.", "difficolta": "intermedio", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-071", "domanda": "C(6,2) combinazioni =", "tipo": "multipla", "opzioni": ["15", "30", "12", "36"], "corretta": 0, "spiegazione": "C(6,2) = 6!/(2!·4!) = 15.", "difficolta": "facile", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-072", "domanda": "∫1/(1+x²) dx =", "tipo": "multipla", "opzioni": ["arctan(x) + c", "arcsin(x) + c", "ln(1+x²) + c", "1/x + c"], "corretta": 0, "spiegazione": "Integrale notevole.", "difficolta": "intermedio", "argomento": "Integrali", "sottoargomento": "Integrale indefinito, e primitive"},
    {"id": "mat-073", "domanda": "lim(x→0) (1-cos x)/x² =", "tipo": "multipla", "opzioni": ["1/2", "1", "0", "2"], "corretta": 0, "spiegazione": "Limite notevole.", "difficolta": "avanzato", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-074", "domanda": "f(x) = x³-3x, punti critici:", "tipo": "multipla", "opzioni": ["x = ±1", "x = 0", "x = 3", "x = ±√3"], "corretta": 0, "spiegazione": "f' = 3x²-3 = 0 → x = ±1.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Teoremi fondamentali calcolo differenziale"},
    {"id": "mat-075", "domanda": "(arcsin x)' =", "tipo": "multipla", "opzioni": ["1/√(1-x²)", "1/(1+x²)", "-1/√(1-x²)", "1/√(1+x²)"], "corretta": 0, "spiegazione": "Derivata dell'arcoseno.", "difficolta": "avanzato", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-076", "domanda": "(arctan x)' =", "tipo": "multipla", "opzioni": ["1/(1+x²)", "1/√(1-x²)", "-1/(1+x²)", "1/√(1+x²)"], "corretta": 0, "spiegazione": "Derivata dell'arcotangente.", "difficolta": "avanzato", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-vf-01", "domanda": "La derivata di una costante è sempre zero.", "tipo": "vero_falso", "corretta": true, "spiegazione": "Costante non varia → derivata = 0.", "difficolta": "facile", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-vf-02", "domanda": "Se f'(x₀) = 0, x₀ è sempre un estremo.", "tipo": "vero_falso", "corretta": false, "spiegazione": "Falso: potrebbe essere un flesso (es. x³).", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Teoremi fondamentali calcolo differenziale"},
    {"id": "mat-vf-03", "domanda": "Continua implica derivabile.", "tipo": "vero_falso", "corretta": false, "spiegazione": "Falso: |x| è continua ma non derivabile in 0.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Continuità, e teoremi fondamentali"},
    {"id": "mat-vf-04", "domanda": "Derivabile implica continua.", "tipo": "vero_falso", "corretta": true, "spiegazione": "Vero: derivabilità → continuità.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Continuità, e teoremi fondamentali"},
    {"id": "mat-vf-05", "domanda": "L'integrale definito può essere negativo.", "tipo": "vero_falso", "corretta": true, "spiegazione": "Vero: area sotto l'asse x è negativa.", "difficolta": "facile", "argomento": "Integrali", "sottoargomento": "Integrale definito, e teorema fondamentale"},
    {"id": "mat-vf-06", "domanda": "log(x) è definito per x < 0.", "tipo": "vero_falso", "corretta": false, "spiegazione": "Falso: log definito solo per x > 0.", "difficolta": "facile", "argomento": "Funzioni reali", "sottoargomento": "Dominio, codominio, e grafico"},
    {"id": "mat-vf-07", "domanda": "0 ≤ P(E) ≤ 1 per ogni evento.", "tipo": "vero_falso", "corretta": true, "spiegazione": "Vero: per definizione.", "difficolta": "facile", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-vf-08", "domanda": "eˣ è sempre positiva.", "tipo": "vero_falso", "corretta": true, "spiegazione": "Vero: eˣ > 0 per ogni x.", "difficolta": "facile", "argomento": "Funzioni reali", "sottoargomento": "Funzioni elementari, e trasformazioni"},
    {"id": "mat-vf-09", "domanda": "lim sin(x)/x per x→0 è 0.", "tipo": "vero_falso", "corretta": false, "spiegazione": "Falso: il limite è 1.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-vf-10", "domanda": "f'' > 0 → f convessa.", "tipo": "vero_falso", "corretta": true, "spiegazione": "Vero: f'' > 0 indica concavità verso l'alto.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Studio completo di funzione"},
    {"id": "mat-vf-11", "domanda": "P(A∩B) = 0 → A e B indipendenti.", "tipo": "vero_falso", "corretta": false, "spiegazione": "Falso: sono incompatibili, non indipendenti.", "difficolta": "avanzato", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-vf-12", "domanda": "(sin 2x)' = 2cos 2x.", "tipo": "vero_falso", "corretta": true, "spiegazione": "Vero: regola della catena.", "difficolta": "intermedio", "argomento": "Derivate, e applicazioni", "sottoargomento": "Definizione, e significato geometrico derivata"},
    {"id": "mat-vf-13", "domanda": "∫[0,1] x dx = 1/2.", "tipo": "vero_falso", "corretta": true, "spiegazione": "Vero: [x²/2]₀¹ = 1/2.", "difficolta": "facile", "argomento": "Integrali", "sottoargomento": "Integrale definito, e teorema fondamentale"},
    {"id": "mat-vf-14", "domanda": "Funzione pari → f(0) = 0.", "tipo": "vero_falso", "corretta": false, "spiegazione": "Falso: cos(x) è pari ma cos(0) = 1.", "difficolta": "intermedio", "argomento": "Funzioni reali", "sottoargomento": "Funzioni elementari, e trasformazioni"},
    {"id": "mat-vf-15", "domanda": "E(X) può essere negativo.", "tipo": "vero_falso", "corretta": true, "spiegazione": "Vero: se X assume valori negativi.", "difficolta": "intermedio", "argomento": "Probabilità, e statistica", "sottoargomento": "Variabili aleatorie, e distribuzioni"},
    {"id": "mat-077", "domanda": "∫1/√(1-x²) dx =", "tipo": "multipla", "opzioni": ["arcsin(x) + c", "arctan(x) + c", "arccos(x) + c", "ln|x| + c"], "corretta": 0, "spiegazione": "Integrale notevole dell'arcoseno.", "difficolta": "avanzato", "argomento": "Integrali", "sottoargomento": "Integrale indefinito, e primitive"},
    {"id": "mat-078", "domanda": "lim(x→0) ln(1+x)/x =", "tipo": "multipla", "opzioni": ["1", "0", "e", "+∞"], "corretta": 0, "spiegazione": "Limite notevole.", "difficolta": "avanzato", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-079", "domanda": "f(x) = eˣ - x - 1, f(0) =", "tipo": "multipla", "opzioni": ["0", "1", "-1", "e"], "corretta": 0, "spiegazione": "f(0) = e⁰ - 0 - 1 = 1 - 1 = 0.", "difficolta": "facile", "argomento": "Funzioni reali", "sottoargomento": "Dominio, codominio, e grafico"},
    {"id": "mat-080", "domanda": "Rolle: f(a)=f(b), f continua e derivabile →", "tipo": "multipla", "opzioni": ["∃c: f'(c)=0", "f costante", "f(c)=0", "f'(a)=f'(b)"], "corretta": 0, "spiegazione": "Esiste punto con tangente orizzontale.", "difficolta": "avanzato", "argomento": "Derivate, e applicazioni", "sottoargomento": "Teoremi fondamentali calcolo differenziale"},
    {"id": "mat-081", "domanda": "∫[0,π] sin x dx =", "tipo": "multipla", "opzioni": ["2", "0", "π", "1"], "corretta": 0, "spiegazione": "[-cos x]₀^π = -(-1) - (-1) = 2.", "difficolta": "intermedio", "argomento": "Integrali", "sottoargomento": "Integrale definito, e teorema fondamentale"},
    {"id": "mat-082", "domanda": "∫[0,1] eˣ dx =", "tipo": "multipla", "opzioni": ["e - 1", "e", "1", "e + 1"], "corretta": 0, "spiegazione": "[eˣ]₀¹ = e - 1.", "difficolta": "facile", "argomento": "Integrali", "sottoargomento": "Integrale definito, e teorema fondamentale"},
    {"id": "mat-083", "domanda": "lim(x→+∞) x/eˣ =", "tipo": "multipla", "opzioni": ["0", "+∞", "1", "e"], "corretta": 0, "spiegazione": "eˣ cresce più velocemente di x.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-084", "domanda": "lim(x→+∞) ln x/x =", "tipo": "multipla", "opzioni": ["0", "+∞", "1", "-∞"], "corretta": 0, "spiegazione": "x cresce più velocemente di ln x.", "difficolta": "intermedio", "argomento": "Limiti, e continuità", "sottoargomento": "Concetto di limite, e calcolo"},
    {"id": "mat-085", "domanda": "f(x) = x² ha minimo in:", "tipo": "multipla", "opzioni": ["x = 0", "x = 1", "x = -1", "Non ha minimo"], "corretta": 0, "spiegazione": "f'(x) = 2x = 0 → x = 0, minimo.", "difficolta": "facile", "argomento": "Derivate, e applicazioni", "sottoargomento": "Studio completo di funzione"},
    {"id": "mat-086", "domanda": "f(x) = -x² ha massimo in:", "tipo": "multipla", "opzioni": ["x = 0", "x = 1", "x = -1", "Non ha massimo"], "corretta": 0, "spiegazione": "f'(x) = -2x = 0 → x = 0, massimo.", "difficolta": "facile", "argomento": "Derivate, e applicazioni", "sottoargomento": "Studio completo di funzione"},
    {"id": "mat-087", "domanda": "6! =", "tipo": "multipla", "opzioni": ["720", "120", "360", "5040"], "corretta": 0, "spiegazione": "6! = 6·5·4·3·2·1 = 720.", "difficolta": "facile", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-088", "domanda": "C(5,2) =", "tipo": "multipla", "opzioni": ["10", "20", "25", "5"], "corretta": 0, "spiegazione": "C(5,2) = 5!/(2!·3!) = 10.", "difficolta": "facile", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-089", "domanda": "Dado: P(numero pari) =", "tipo": "multipla", "opzioni": ["1/2", "1/3", "1/6", "2/3"], "corretta": 0, "spiegazione": "3 numeri pari su 6: 3/6 = 1/2.", "difficolta": "facile", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"},
    {"id": "mat-090", "domanda": "Due dadi: P(somma = 7) =", "tipo": "multipla", "opzioni": ["1/6", "1/12", "1/36", "7/36"], "corretta": 0, "spiegazione": "6 combinazioni su 36: 6/36 = 1/6.", "difficolta": "intermedio", "argomento": "Probabilità, e statistica", "sottoargomento": "Calcolo combinatorio, e probabilità"}
  ]
}
